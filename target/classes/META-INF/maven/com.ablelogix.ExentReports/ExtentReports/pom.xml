   		<version>0.0.1-SNAPSHOT</version>
	</dependency>

	<dependency>
    	<groupId>com.ablelogix.Orange_Hrm</groupId>
    	<artifactId>TDD_Framework</artifactId>
    	<version>0.0.1-SNAPSHOT</version>
	</dependency>
	
	<dependency>
		<groupId>com.ablelogix.Orange_Hrm</groupId>
		<artifactId>TDD_Framework</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</dependency>
	
	<dependency>
